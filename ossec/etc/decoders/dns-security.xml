<decoder name="dns-security">
  <program_name>named</program_name>
</decoder>

<decoder name="dns-tunneling-attempt">
  <parent>dns-security</parent>
  <regex>client (\S+)#(\d+): query: (.+) (\S+) (\S+)</regex>
  <order>client_ip, client_port, domain, query_type, query_class</order>
</decoder>

<decoder name="dns-dga-detection">
  <parent>dns-security</parent>
  <regex>client (\S+)#(\d+): query: (.+) (\S+) (\S+)</regex>
  <order>client_ip, client_port, domain, query_type, query_class</order>
</decoder>

<decoder name="dns-zone-transfer-attempt">
  <parent>dns-security</parent>
  <regex>client (\S+)#(\d+): query: (\S+) (\S+) (\S+)</regex>
  <order>client_ip, client_port, domain, query_type, query_class</order>
</decoder>

<decoder name="dns-reconnaissance">
